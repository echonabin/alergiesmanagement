{
  "name": "alergiesmanagement",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:backend": "cross-env NODE_ENV=development nx serve alergies-backend",
    "start:ui": "nx serve alergies-ui",
    "ui:lint": "nx run alergies-ui:lint",
    "backend:lint": "nx run alergies-backend:lint",
    "e2e:ui": "nx run alergies-ui-e2e:e2e",
    "build": "nx build",
    "test": "cross-env NODE_ENV=test nx test --runInBand --coverage",
    "migrate:make": "knex migrate:make --migrations-directory apps/alergies-backend/src/db/migrations -x ts",
    "migrate:latest": "knex migrate:latest --knexfile apps/alergies-backend/src/db/knexfile.ts",
    "migrate:rollback": "knex migrate:rollback --knexfile apps/alergies-backend/src/db/knexfile.ts",
    "migrate:up": "knex migrate:up --knexfile apps/alergies-backend/src/db/knexfile.ts",
    "migrate:down": "knex migrate:down --knexfile apps/alergies-backend/src/db/knexfile.ts"
  },
  "private": true,
